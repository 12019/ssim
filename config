;************************************************************
; SSIM BUILD CONFIGURATION
;************************************************************

;------------------------------------------------------------
; Basic issues
;------------------------------------------------------------
; Set processor type to PIC 16F876
	PROCESSOR	16f876
	LIST		p=16f876

; Config bits
;	__CONFIG	_CP_HALF & _DEBUG_OFF & _CPD_ON & _LVP_OFF & _BODEN_OFF & _PWRTE_OFF & _WRT_ENABLE_ON & _WDT_OFF & _XT_OSC
	__CONFIG	H'1FDF'  & H'3FFF'    & H'3EFF' &  H'3F7F' & H'3FBF'    & H'3FFF'    & H'3FFF'        & H'3FFB'  & H'3FFD'

; Define if program uses interrupts (reserved for future use)
; #DEFINE InterruptUse

;------------------------------------------------------------
; Card settings
;------------------------------------------------------------
; ICCID
#define	CONFIG_ICCID		0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF
; PIN (default 1234)
#define CONFIG_PIN              0x31, 0x32, 0x33, 0x34, 0xFF, 0xFF, 0xFF, 0xFF
; PUK (default 12345678)
#define CONFIG_PUK              0x31, 0x32, 0x33, 0x34, 0x35, 0x36, 0x37, 0x38
; PIN2  (default 1234)
#define CONFIG_PIN2             0x31, 0x32, 0x33, 0x34, 0xFF, 0xFF, 0xFF, 0xFF
; PUK2  (default 12345678)
#define CONFIG_PUK2             0x31, 0x32, 0x33, 0x34, 0x35, 0x36, 0x37, 0x38

;------------------------------------------------------------
; Input data check ranges - see source, normally no need to modify
;------------------------------------------------------------
#define	ASIM_RANGE_CHECK	0x07	; no of sims (bitmask)
#define	LANG_RANGE_CHECK	0x01	; no of languages (bitmask)

;------------------------------------------------------------
; SIM settings
;------------------------------------------------------------
; Number of accounts present
#define CONFIG_NO_SIM_PRESENT	0x03

; Account names
#define	SIM_00_NAME		"EUROTEL"
#define	SIM_01_NAME		"PAEGAS"
#define	SIM_02_NAME		"OSKAR"
#define	SIM_03_NAME		""
#define	SIM_04_NAME		""
#define	SIM_05_NAME		""
#define	SIM_06_NAME		""
#define	SIM_07_NAME		""

; IMSI
#define	CONFIG_IMSI_00		0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00
