; SMSP
#define	EUROTEL_SMSP	0xE9, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0x07, 0x91, 0x24, 0x60, 0x20, 0x09, 0x99, 0x90, 0xFF, 0xFF, 0xFF, 0xFF, 0x00, 0xFF, 0xAD ; EUROTEL CZ
#define PAEGAS_SMSP	0xE9, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0x07, 0x91, 0x24, 0x60, 0x30, 0x50, 0x02, 0x00, 0xFF, 0xFF, 0xFF, 0xFF, 0x00, 0xFF, 0xA9 ; PAEGAS CZ / T-MOBILE CZ
#define OSKAR_SMSP	0xE9, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0x07, 0x91, 0x24, 0x60, 0x80, 0x00, 0x65, 0x18, 0xFF, 0xFF, 0xFF, 0xFF, 0x00, 0xFF, 0xFF ; OSKAR

#define CONFIG_SMSP_00	EUROTEL_SMSP
#define CONFIG_SMSP_01	PAEGAS_SMSP
#define CONFIG_SMSP_02	OSKAR_SMSP
#define CONFIG_SMSP_03	EUROTEL_SMSP
#define CONFIG_SMSP_04	EUROTEL_SMSP
#define CONFIG_SMSP_05	EUROTEL_SMSP
#define CONFIG_SMSP_06	EUROTEL_SMSP
#define CONFIG_SMSP_07	EUROTEL_SMSP

;------------------------------------------------------------
; Auto Morph Patterns
;------------------------------------------------------------
; card number to perform specified calls (0x00..0x0F)
#define	CONFIG_EUROTEL_CALLS	0x00
#define	CONFIG_PAEGAS_CALLS	0x01
#define	CONFIG_OSKAR_CALLS	0x02

; number of amp patterns - don't change - it is not as easy as it seems
#define	CONFIG_AMP_COUNT	0x14

#define	CONFIG_AMP_01	0x8F,				0x91, 0x88, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +88 - SSIM function call
#define	CONFIG_AMP_02	0x80 | CONFIG_EUROTEL_CALLS, 	0x91, 0x24, 0x60, 0x10, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420601 - EUROTEL
#define	CONFIG_AMP_03	0x80 | CONFIG_EUROTEL_CALLS, 	0x91, 0x24, 0x60, 0x20, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420602 - EUROTEL
#define	CONFIG_AMP_04	0x80 | CONFIG_PAEGAS_CALLS , 	0x91, 0x24, 0x60, 0x30, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420603 - PAEGAS
#define	CONFIG_AMP_05	0x80 | CONFIG_PAEGAS_CALLS , 	0x91, 0x24, 0x60, 0x40, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420604 - PAEGAS
#define	CONFIG_AMP_06	0x80 | CONFIG_PAEGAS_CALLS , 	0x91, 0x24, 0x60, 0x50, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420605 - PAEGAS
#define	CONFIG_AMP_07	0x80 | CONFIG_EUROTEL_CALLS, 	0x91, 0x24, 0x60, 0x60, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420606 - EUROTEL
#define	CONFIG_AMP_08	0x80 | CONFIG_EUROTEL_CALLS, 	0x91, 0x24, 0x60, 0x70, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420607 - EUROTEL
#define	CONFIG_AMP_09	0x80 | CONFIG_OSKAR_CALLS  , 	0x91, 0x24, 0x60, 0x80, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +420608 - OSKAR
#define	CONFIG_AMP_10	0x80 | CONFIG_EUROTEL_CALLS, 	0x91, 0x24, 0x70, 0xF2, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +42072X - EUROTEL
#define	CONFIG_AMP_11	0x80 | CONFIG_PAEGAS_CALLS , 	0x91, 0x24, 0x70, 0xF3, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +42073X - PAEGAS
#define	CONFIG_AMP_12	0x80 | CONFIG_OSKAR_CALLS  , 	0x91, 0x24, 0x70, 0xF7, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF	; +42077X - OSKAR
#define	CONFIG_AMP_13	0x00, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF
#define	CONFIG_AMP_14	0x00, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF
