;------------------------------------------------------------ 
; [A] '6F42' - EF SMSP (Short message service parameters)
;------------------------------------------------------------ 

	data	0x00, 0x1C			; size
	data	FSTATUS_NOT_INV | FSTRUC_LIN	; status + structure
	data	0x11, 0xFF, 0xFF		; access conditions
	data	0x01				; number of records
	data	0x1C				; length of record

	data	CONFIG_SMSP
